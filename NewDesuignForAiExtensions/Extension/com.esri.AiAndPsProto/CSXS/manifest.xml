<?xml version="1.0" encoding="UTF-8"?>
<ExtensionManifest Version="5.0" ExtensionBundleId="com.esri.AiAndPsProto" ExtensionBundleVersion="1.0.0"
		ExtensionBundleName="ArcGIS" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<ExtensionList>
		<Extension Id="com.esri.AiAndPsProto" Version="1.0" />
		<Extension Id="com.esri.AiAndPsProto.dialog" Version="1.0" />
	</ExtensionList>
	<ExecutionEnvironment>
		<HostList>
            <!-- Uncomment Host tags according to the apps you want your panel to support -->
            <!-- Photoshop -->

            <Host Name="PHXS" Version="[15.0,15.9]" />
            <Host Name="PHSP" Version="[15.0,15.9]" />

            <!-- Illustrator -->
            <Host Name="ILST" Version="[18.0,18.9]" />
            <!-- InDesign -->
            <!-- <Host Name="IDSN" Version="[10.0,10.9]" /> -->          
            <!-- Premiere -->
            <!-- <Host Name="PPRO" Version="[8.0,8.9]" /> -->
            <!-- AfterEffects -->
            <!-- <Host Name="AEFT" Version="[13.0,13.9]" /> -->
            <!-- PRELUDE -->
            <!-- <Host Name="PRLD" Version="[3.0,3.9]" />   -->
            <!-- FLASH Pro -->
            <!-- <Host Name="FLPR" Version="[14.0,14.9]" /> -->

		</HostList>
		<LocaleList>
			<Locale Code="All" />
		</LocaleList>
		<RequiredRuntimeList>
			<RequiredRuntime Name="CSXS" Version="5.0" />
		</RequiredRuntimeList>
	</ExecutionEnvironment>
	<DispatchInfoList>
		<Extension Id="com.esri.AiAndPsProto.dialog">
			<DispatchInfo >
				<Resources>
				<MainPath>./dialog.html</MainPath>
				<CEFCommandLine>
					<Parameter>--nodejs-disabled</Parameter>
				</CEFCommandLine>
				</Resources>
				<Lifecycle>
					<AutoVisible>true</AutoVisible>
				</Lifecycle>
				<UI>
					<Type>ModalDialog</Type>
					<Geometry>
						<Size>
							<Height>300</Height>
							<Width>900</Width>
						</Size>
						<MinSize>
							<Height>300</Height>
							<Width>900</Width>
						</MinSize>
					</Geometry>
				</UI>
			</DispatchInfo>
		</Extension>
		<Extension Id="com.esri.AiAndPsProto">
			<DispatchInfo >
				<Resources>
				<MainPath>./index.html</MainPath>
				<ScriptPath>./jsx/hostscript.jsx</ScriptPath>
				</Resources>
				<Lifecycle>
					<AutoVisible>true</AutoVisible>
				</Lifecycle>
				<UI>
					<Type>Panel</Type>
					<Menu>ArcGIS maps for Adobe Creative Cloud</Menu>
					<Geometry>
						<Size>
							<Height>600</Height>
							<Width>800</Width>
						</Size>
                    <MinSize>
                            <Height>600</Height>
                            <Width>800</Width>
                        </MinSize>
                    <MaxSize>
                            <Height>600</Height>
                            <Width>800</Width>
                    </MaxSize>
                        
					</Geometry>
					<Icons>
						<Icon Type="Normal">./icons/ArcGISConnection48.png</Icon>
						<Icon Type="RollOver">./icons/ArcGISConnection48.png</Icon>
						<Icon Type="Disabled">./icons/ArcGISConnection48BW.png</Icon>
						<Icon Type="DarkNormal">./icons/ArcGISConnection48BW.png</Icon>
						<Icon Type="DarkRollOver">./icons/ArcGISConnection48BW.png</Icon>
					</Icons>					
				</UI>
			</DispatchInfo>
		</Extension>
	</DispatchInfoList>
</ExtensionManifest>