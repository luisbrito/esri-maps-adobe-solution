<?xml version="1.0" encoding="UTF-8"?>

<ExtensionManifest Version="5.0" ExtensionBundleId="com.example.AutorizationPrototype" ExtensionBundleVersion="1.0.0"
		ExtensionBundleName="AutorizationPrototype" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<ExtensionList>
		<Extension Id="com.example.AutorizationPrototype.dialog" Version="1.0" />
		<Extension Id="com.example.AutorizationPrototype.extension1" Version="1.0" />
	</ExtensionList>
	<ExecutionEnvironment>
		<HostList>
			<Host Name="ILST" Version="[18.0,18.9]" />
		</HostList>
		<LocaleList>
			<Locale Code="All" />
		</LocaleList>
		<RequiredRuntimeList>
			<RequiredRuntime Name="CSXS" Version="5.0" />

		</RequiredRuntimeList>
	</ExecutionEnvironment>
	<DispatchInfoList>
		<Extension Id="com.example.AutorizationPrototype.dialog">
			<DispatchInfo >
				<Resources>
				<MainPath>./dialog.html</MainPath>
				<CEFCommandLine>
					<Parameter>--nodejs-disabled</Parameter>
				</CEFCommandLine>
				</Resources>
				<Lifecycle>
					<AutoVisible>true</AutoVisible>
				</Lifecycle>
				<UI>
					<Type>ModalDialog</Type>
					<Geometry>
						<Size>
							<Height>500</Height>
							<Width>300</Width>
						</Size>
						<MinSize>
							<Height>500</Height>
							<Width>300</Width>
						</MinSize>
					</Geometry>
				</UI>
			</DispatchInfo>
		</Extension>
		<Extension Id="com.example.AutorizationPrototype.extension1">
			<DispatchInfo >
				<Resources>
				<MainPath>./index.html</MainPath>
				</Resources>
				<Lifecycle>
					<AutoVisible>true</AutoVisible>
				</Lifecycle>
				<UI>
					<Type>Panel</Type>
					<Menu>ArcGIS authorization prototype</Menu>
					<Geometry>
						<Size>
							<Height>200</Height>
							<Width>400</Width>
						</Size>
						<MaxSize>
							<Height>200</Height>
							<Width>400</Width>
						</MaxSize>
					</Geometry>
				</UI>
			</DispatchInfo>
		</Extension>
	</DispatchInfoList>
</ExtensionManifest>